select s1, b1, s2 from BuySellEvent#ext_timed(stock_time, TIMESTAMP milliseconds)
match_recognize (
measures s1 as s1, b1 as b1, s2 as s2
all matches
after match skip to current row
pattern (s1 s* b1 (b1 | s)* s2)
define
    s1 as s1.type = "SELL" and s1.name = "MSFT",
    b1 as (b1.type = "BUY" or b1.type = "SELL") and b1.name = "QQQ" and b1.volume = 4000,
    s2 as s2.type = "SELL" and s2.name = "AMAT" and s2.price < s1.price)